<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>159.65.111.117_website</groupId>
  <artifactId>159.65.111.117_website</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <build>
    <sourceDirectory>tomcat/apache-tomcat-9.0.30/webapps/docs/appdev/sample/src/mypackage</sourceDirectory>
    <resources>
      <resource>
        <directory>tomcat/apache-tomcat-9.0.30/webapps/examples/WEB-INF/classes</directory>
        <excludes>
          <exclude>**/*.java</exclude>
        </excludes>
      </resource>
    </resources>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.8.0</version>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <dependencies>
  	<dependency>
  		<groupId>net.ricecode</groupId>
  		<artifactId>string-similarity</artifactId>
  		<version>1.0.0</version>
  	</dependency>
  </dependencies>
</project>